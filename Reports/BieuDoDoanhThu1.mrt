        <Name>DanhSachSanPham</Name>
        <NameInSource>DanhSachSanPham</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT 
	CAST(hd.NgayLap AS DATE) AS NgayLap,
	SUM(ctd.SoLuongBan * ctd.DonGiaBan) AS ThanhTien
